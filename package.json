{
  "name": "koawheel",
  "app": "bin/www",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node $npm_package_app",
    "debug": "./node_modules/.bin/nodemon $npm_package_app",
    "stop": "pkill -9 node",
    "test": "node $npm_package_app --test",
    "zip": "node ./bin/zip.js -n serve.zip",
    "pm2": "pm2 start ecosystem.config.js --env dev",
    "pm2.json": "pm2-runtime start pm2.json",
    "prod": "pm2 start ecosystem.config.js --env production",
    "resetDB": "node bin/database",
    "doc": "apidoc -i src/controllers -o doc",
    "deploy": "node bin/deploy.js",
    "lint": "standard --fix"
  },
  "skPlugins": [
    {
      "src": "@/plugins/redis",
      "enable": false
    },
    {
      "src": "@/plugins/model",
      "enable": false
    }
  ],
  "skMiddlewares": [
    {
      "src": "@/middlewares/cors"
    },
    {
      "src": "@/middlewares/alive"
    },
    {
      "src": "@/middlewares/bodyparser"
    },
    {
      "src": "koa-json"
    },
    {
      "src": "@/middlewares/log4"
    },
    {
      "src": "@/middlewares/URLPrefix",
      "options": {
        "prefix": "/api"
      }
    },
    {
      "src": "@/middlewares/responseFormatter"
    },
    {
      "src": "@/middlewares/jwt"
    }
  ],
  "engines": {
    "node": ">=12.13.0"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@": "src"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/register": "^7.8.6",
    "commander": "^2.20.3",
    "debug": "^2.6.3",
    "glob": "^7.1.6",
    "ioredis": "^4.16.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-cors": "^2.0.6",
    "log4js": "^3.0.6",
    "module-alias": "^2.2.2",
    "mysql2": "^1.7.0",
    "pug": "^2.0.4",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "archiver": "^3.1.1",
    "node-ssh": "^6.0.0",
    "nodemon": "^1.19.4"
  }
}
